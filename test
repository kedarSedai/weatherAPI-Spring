 private final ByteArrayOutputStream outContent = new ByteArrayOutputStream();
    private final PrintStream originalOut = System.out;

    @Before
    public void setUpStreams() {
        System.setOut(new PrintStream(outContent));
    }

    @After
    public void restoreStreams() {
        System.setOut(originalOut);
    }

    @Test
    public void testUtil() throws Exception {
        // Mock the ObjectMapper
        ObjectMapper objectMapperMock = Mockito.mock(ObjectMapper.class);
        JsonNode rootNodeMock = Mockito.mock(JsonNode.class);
        JsonNode apisNodeMock = Mockito.mock(JsonNode.class);
        JsonNode srcIdNodeMock = Mockito.mock(JsonNode.class);
        JsonNode fltsNodeMock = Mockito.mock(JsonNode.class);
        JsonNode fltNodeMock = Mockito.mock(JsonNode.class);

        // Provide a sample JSON for testing
        String sampleJson = "{ \"APIS\": [ { \"APIS_INTERNAL\": 1, \"APIS_FLTS\": [ { \"APIS_TECS_ID\": \"00035\" }, { \"APIS_TECS_ID\": \"0094\" } ] } ], \"SRCE_TYP\": \"APIS\", \"SRC_ID\": [ \"00035\", \"00414\", \"00314\" ] }";

        // Mock behavior of ObjectMapper methods
        when(objectMapperMock.readTree(sampleJson)).thenReturn(rootNodeMock);
        when(rootNodeMock.get("APIS")).thenReturn(apisNodeMock);
        when(rootNodeMock.get("SRC_ID")).thenReturn(srcIdNodeMock);
        when(srcIdNodeMock.elements()).thenReturn(() -> srcIdNodeMock.elements());

        when(apisNodeMock.size()).thenReturn(1);
        when(apisNodeMock.get(0)).thenReturn(apisNodeMock);
        when(apisNodeMock.get("APIS_FLTS")).thenReturn(fltsNodeMock);
        when(fltsNodeMock.size()).thenReturn(2);
        when(fltsNodeMock.get(0)).thenReturn(fltNodeMock);
        when(fltsNodeMock.get(1)).thenReturn(fltNodeMock);
        when(fltNodeMock.get("APIS_TECS_ID")).thenReturn(fltNodeMock);
        when(fltNodeMock.asText()).thenReturn("00035", "0094");

        // Create an instance of the class that contains the util method
        YourClass yourClass = new YourClass();

        // Call the util method with the mocked ObjectMapper
        yourClass.util(objectMapperMock, sampleJson);

        // Verify the output to the console
        String expectedOutput = "Matching APIS_TECS_ID: 00035\nMatching APIS_TECS_ID: 0094\n";
        assertEquals(expectedOutput, outContent.toString());
    }
